    "manifest_version": 1,

    "description": "Track your daily completions.",
    "dependencies": {
        "bh.blishhud": ">=0.11.0"
    },
    "url": "https://github.com/manlaan/Blishhud-Dailies",
    "contributors": [
        {
            "name": "Manlaan",
            "username": "manlaan.8921"
        }
    ],
    "api_permissions": {
        "account": {
